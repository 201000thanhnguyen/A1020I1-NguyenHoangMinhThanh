{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "11fd5557-7584-4a6a-84f8-aff54fc450a6"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "FuramaNew": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/FuramaNew",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/open-iconic/font/css/open-iconic-bootstrap.min.css",
              "node_modules/animate.css/animate.css",
              "node_modules/owl.carousel/dist/assets/owl.carousel.css",
              "node_modules/owl.carousel/dist/assets/owl.theme.default.css",
              "node_modules/magnific-popup/dist/magnific-popup.css",
              "src/assets/lib_of_mt/aos.css",
              "node_modules/ionicons/dist/css/ionicons.css",
              "node_modules/jquery-datetimepicker/jquery.datetimepicker.css",
              "node_modules/icomoon/style.css",
              "src/assets/lib_of_mt/dataTables.bootstrap4.min.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "src/assets/lib_of_mt/style.css",
              "src/styles.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "src/assets/lib_of_mt/jquery-migrate-3.0.1.min.js",
              "node_modules/popper.js/dist/umd/popper.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "node_modules/jquery.easing/jquery.easing.js",
              "node_modules/waypoints/lib/jquery.waypoints.js",
              "node_modules/jquery.stellar/jquery.stellar.js",
              "node_modules/owl.carousel/dist/owl.carousel.js",
              "node_modules/magnific-popup/dist/jquery.magnific-popup.js",
              "src/assets/lib_of_mt/aos.js",
              "node_modules/jquery.animate-number/jquery.animateNumber.js",
              "node_modules/jquery-datetimepicker/jquery.datetimepicker.js",
              "src/assets/lib_of_mt/scrollax.min.js",
              "node_modules/datatables.net/js/jquery.dataTables.js",
              "src/assets/lib_of_mt/dataTables.bootstrap4.min.js",
              "src/assets/lib_of_mt/main.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "FuramaNew:build:production"
            },
            "development": {
              "browserTarget": "FuramaNew:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "FuramaNew:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "FuramaNew"
}
